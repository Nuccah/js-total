#places
  background: white;

  .spinner
    width 32px
    height 32px
    margin-right 1em
    vertical-align middle

  li
    &.searching
      height 32px
      min-height 0
      text-align center
      font-size 16px
      line-height 32px
      font-style italic
      color silver

    &.active .details
      background #0091d4
      color white
      small
        color #ddd

#checkInUI button[type="submit"]
  width 100%
